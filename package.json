{
  "name": "sorting-animation",
  "version": "2.10.2",
  "description": "sorting algorithm animation",
  "author": "Lin fang",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --mode development --host 0.0.0.0",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf build",
    "build": "npm run clean && export SET NODE_OPTIONS=--openssl-legacy-provider && webpack --mode production && cp -R error build/ && npx copyfiles -u 1 ./src/icons/icon.png ./build/",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "npm": "9.6.7",
    "node": "18.17.1"
  },
  "dependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/git": "^10.0.1",
    "@svgr/webpack": "^5.5.0",
    "@types/codemirror": "0.0.98",
    "@types/css-font-loading-module": "0.0.4",
    "@types/history": "^4.7.8",
    "@types/lodash": "^4.14.162",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.19",
    "@types/ua-parser-js": "^0.7.36",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "codemirror": "^5.58.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.0.0",
    "eslint": "^8.29.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.1.1",
    "history": "^5.0.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^8.0.3",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "postcss": "^8.1.4",
    "postcss-loader": "^4.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "semantic-release": "^22.0.0",
    "source-map-loader": "^1.1.1",
    "style-loader": "^2.0.0",
    "styled-components": "^5.3.3",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.1.0",
    "typescript": "^4.5.4",
    "ua-parser-js": "^1.0.32",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "preset": "conventionalcommits"
  }
}
